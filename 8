#include <stdio.h>

int main() {
    int n, i;
    unsigned long long factorial = 1;

    printf("정수를 입력하세요: ");
    scanf("%d", &n);

    if (n < 0)
        printf("음수의 팩토리얼은 존재하지 않습니다.\n");
    else {
        for (i = 1; i <= n; ++i) {
            factorial *= i;
        }
        printf("%d의 팩토리얼: %llu\n", n, factorial);
    }

    return 0;
}
